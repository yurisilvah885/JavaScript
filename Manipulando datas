let data= new Date()
console.log (data)

var ano= data.getFullYear()
console.log(ano)

var mes=data.getMonth()
console.log(mes)

const mesesdoano= ['janeiro', 'fevereiro', 'março', 'abril', 'maio', 'junho', 'julho', 'agosto', 'setembro', 'outubro', 'novembro', 'dezembro']
var mesescrito= mesesdoano[data.getMonth()]
console.log(mesescrito)

var diames= data.getDate()
console.log(diames )

var diasemana= data.getDay()
console.log(diasemana)

const semana= ['domingo', 'segunda', 'terça', 'quarta', 'quinta', 'sexta', 'sabado']
var sem= semana[data.getDay()]
console.log(sem)

var minutos = data.getMinutes()
console.log(minutos)

var dataBR= data.toLocaleDateString('pt-BR')
console.log(dataBR)



var datai= new Date()
var dataf= new Date(2023,11,25)

var diferença= datai.getTime()- dataf.getTime()
var dias= Math.ceil(diferença/(24*60*60*1000))
console.log(dias)

var hora= data.getHours()
console.log(hora)
